{% import "forms/macros.html.j2" as render_form %}
<form hx-post="{{ url_for("create_contact_htmx") }}"
      hx-target="#contact-list">
  <table>
    <tbody>
      {{ render_form.as_rows(form) }}
      <tr>
        <td>
          <button type="submit">
            Add Contact
          </button>
        </td>
      </tr>
    </tbody>
  </table>
</form>
{{ render_form.form_errors(form) }}
